{
  "version": 1,
  "projects": {
    "angular": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular",
      "sourceRoot": "apps/angular/src",
      "prefix": "dry-run",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular",
            "index": "apps/angular/src/index.html",
            "main": "apps/angular/src/main.ts",
            "polyfills": "apps/angular/src/polyfills.ts",
            "tsConfig": "apps/angular/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/angular/src/favicon.ico",
              "apps/angular/src/assets"
            ],
            "styles": ["apps/angular/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular/src/environments/environment.ts",
                  "with": "apps/angular/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular/tsconfig.app.json",
              "apps/angular/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/angular/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/angular/jest.config.js",
            "tsConfig": "apps/angular/tsconfig.spec.json",
            "setupFile": "apps/angular/src/test-setup.ts"
          }
        }
      }
    },
    "angular-e2e": {
      "root": "apps/angular-e2e",
      "sourceRoot": "apps/angular-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-e2e/cypress.json",
            "tsConfig": "apps/angular-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/angular-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/angular-e2e/**"]
          }
        }
      }
    },
    "react": {
      "root": "apps/react",
      "sourceRoot": "apps/react/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/react",
            "index": "apps/react/src/index.html",
            "main": "apps/react/src/main.tsx",
            "polyfills": "apps/react/src/polyfills.ts",
            "tsConfig": "apps/react/tsconfig.app.json",
            "assets": ["apps/react/src/favicon.ico", "apps/react/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/babel"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/react/src/environments/environment.ts",
                  "with": "apps/react/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "react:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "react:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react/.eslintrc",
            "tsConfig": [
              "apps/react/tsconfig.app.json",
              "apps/react/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/react/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/react/jest.config.js",
            "tsConfig": "apps/react/tsconfig.spec.json"
          }
        }
      }
    },
    "react-e2e": {
      "root": "apps/react-e2e",
      "sourceRoot": "apps/react-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/react-e2e/cypress.json",
            "tsConfig": "apps/react-e2e/tsconfig.e2e.json",
            "devServerTarget": "react:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "react:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/react-e2e/.eslintrc",
            "tsConfig": ["apps/react-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/react-e2e/**"]
          }
        }
      }
    },
    "feature-list": {
      "projectType": "library",
      "root": "libs/feature-list",
      "sourceRoot": "libs/feature-list/src",
      "prefix": "dry-run",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/feature-list/tsconfig.lib.json",
              "libs/feature-list/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/feature-list/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/feature-list/jest.config.js",
            "tsConfig": "libs/feature-list/tsconfig.spec.json",
            "setupFile": "libs/feature-list/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/utils/.eslintrc",
            "tsConfig": [
              "libs/utils/tsconfig.lib.json",
              "libs/utils/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/utils/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "tsConfig": "libs/utils/tsconfig.spec.json"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "angular"
}
